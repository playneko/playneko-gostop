leAuth} />} />\n        }\n        <Route path=\"/game/room\" render={() => <GameRoom>{authInfo}</GameRoom>} />\n        </Switch>\n      </ThemeProvider>\n    </Router>\n  );\n}\n\nexport default App;","/Volumes/BackupHDD/work/react/playneko-gostop/src/index.js",[],"/Volumes/BackupHDD/work/react/playneko-gostop/src/component/Login.js",[],"/Volumes/BackupHDD/work/react/playneko-gostop/src/component/Firebase.js",[],"/Volumes/BackupHDD/work/react/playneko-gostop/src/component/Lobby.js",[],"/Volumes/BackupHDD/work/react/playneko-gostop/src/component/CheckLogin.js",[],"/Volumes/BackupHDD/work/react/playneko-gostop/src/component/GameRoom.js",["37","38","39","40","41","42"],{"ruleId":"43","severity":1,"message":"44","line":2,"column":10,"nodeType":"45","messageId":"46","endLine":2,"endColumn":20},{"ruleId":"43","severity":1,"message":"47","line":46,"column":19,"nodeType":"45","messageId":"46","endLine":46,"endColumn":29},{"ruleId":"43","severity":1,"message":"48","line":7,"column":8,"nodeType":"45","messageId":"46","endLine":7,"endColumn":18},{"ruleId":"43","severity":1,"message":"49","line":50,"column":25,"nodeType":"45","messageId":"46","endLine":50,"endColumn":41},{"ruleId":"43","severity":1,"message":"50","line":56,"column":22,"nodeType":"45","messageId":"46","endLine":56,"endColumn":35},{"ruleId":"43","severity":1,"message":"51","line":61,"column":9,"nodeType":"45","messageId":"46","endLine":61,"endColumn":16},{"ruleId":"52","severity":1,"message":"53","line":157,"column":15,"nodeType":"54","endLine":157,"endColumn":75},{"ruleId":"52","severity":1,"message":"53","line":166,"column":11,"nodeType":"54","endLine":166,"endColumn":59},"no-unused-vars","'useHistory' is defined but never used.","Identifier","unusedVar","'setMyTheme' is assigned a value but never used.","'CheckLogin' is defined but never used.","'setBoardCardList' is assigned a value but never used.","'setMyCardList' is assigned a value but never used.","'classes' is assigned a value but never used.","jsx-a1